{
  "name": "create-react-app",
  "version": "0.1.0",
  "private": true,
  "pre-commit": "lint",
  "proxy": "http://172.16.0.182:8899",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd-mobile": "^2.3.3",
    "babel-plugin-import": "^1.13.0",
    "customize-cra": "^1.0.0",
    "dva": "^2.4.1",
    "rc-form": "^2.4.11",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.6",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "lint": "eslint --ext .js --ext .jsx src/",
    "lint-fix": "eslint --fix --ext .js --ext .jsx src/"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "class-methods-use-this": 0,
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "comma-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "no-cond-assign": 0,
      "no-console": 0,
      "no-nested-ternary": 0,
      "no-restricted-syntax": 0,
      "no-throw-literal": 0,
      "semi": [
        2,
        "always"
      ],
      "space-before-function-paren": [
        2,
        "always"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "pre-commit": "^1.2.2"
  }
}
